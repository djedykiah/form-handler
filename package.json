{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "rules": {
      "linebreak-style": [
        0,
        "error",
        "windows"
      ],
      "jsx-a11y/no-autofocus": 0,
      "implicit-arrow-linebreak": 0,
      "no-control-regex": 0,
      "react/destructuring-assignment": 0,
      "operator-linebreak": [
        "error",
        "before",
        {
          "overrides": {
            "=": "ignore"
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
